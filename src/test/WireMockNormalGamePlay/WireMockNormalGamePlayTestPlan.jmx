                <JSR223PreProcessor guiclass="TestBeanGUI" testclass="JSR223PreProcessor" testname="GenerateRoundID" enabled="true">
                  <stringProp name="cacheKey">true</stringProp>
                  <stringProp name="filename"></stringProp>
                  <stringProp name="parameters"></stringProp>
                  <stringProp name="script">/*
 * JMeter JSR223 PreProcessor (Groovy)
 * Purpose: Generate a unique, ordered, thread-safe 10-digit roundId (long) with rollover at 9999999999 and random reseeding
 * ---------------------------------
 * Benefits:
	✅ RoundID will be a 10-digit long
	✅ Globally unique (even across threads)
	✅ Monotonically increasing (always bigger than the previous one)
	✅ Thread-safe (no race conditions in JMeter threads)
	✅ Avoids BigDecimal issues (everything is handled as long)
 */

import java.util.concurrent.atomic.AtomicLong;

// Initialize a global AtomicLong counter if not already created
if (props.get(&quot;globalRoundIdCounter&quot;) == null) {
    // Start with current time truncated to 10 digits
    long initialValue = System.currentTimeMillis() % 10000000000L;
    props.put(&quot;globalRoundIdCounter&quot;, new AtomicLong(initialValue));
}

// Get reference to global counter
AtomicLong counter = (AtomicLong) props.get(&quot;globalRoundIdCounter&quot;);

// Generate next unique roundId (10 digits, long)
long roundId = counter.incrementAndGet();

// Ensure roundId is always 10 digits (wrap around if exceeded)
if (roundId &gt; 9999999999L) {
    // Reset safely to current timestamp last 10 digits
    roundId = System.currentTimeMillis() % 10000000000L;
    props.put(&quot;globalRoundIdCounter&quot;, new AtomicLong(roundId));
}

// Save into JMeter variables
vars.put(&quot;roundId&quot;, Long.toString(roundId));

// log to jmeter.log for debugging
log.info(&quot;Thread Number: ${__threadNum} - Generated RoundID: &quot; + roundId);
</stringProp>
